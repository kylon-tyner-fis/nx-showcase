{
  "name": "teacher",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "frontend/teacher/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/frontend/teacher",
        "index": "frontend/teacher/src/index.html",
        "baseHref": "/",
        "main": "frontend/teacher/src/main.ts",
        "tsConfig": "frontend/teacher/tsconfig.app.json",
        "assets": [
          "frontend/teacher/src/favicon.ico",
          "frontend/teacher/src/assets"
        ],
        "styles": ["frontend/teacher/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "frontend/teacher/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "frontend/teacher/src/environments/environment.ts",
              "with": "frontend/teacher/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "frontend/teacher/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "teacher:build",
        "hmr": true,
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "teacher:build:development"
        },
        "production": {
          "buildTarget": "teacher:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nx/react:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "teacher:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "teacher:serve:development"
        },
        "production": {
          "serveTarget": "teacher:serve:production"
        }
      }
    }
  }
}
